@model GridLogik.ViewModels.prmglobal

@{
    ViewBag.Title = "Create";
}


<section>
    <div id="main-content">
        <div class="container-fluid">
            <div class="row">
                <div class="col-md-12">
                    <ol class="breadcrumb">
                        <li><a href="#"><i class="fa fa-home"></i></a></li>
                        <li><a href="#">Configurations</a></li>
                        <li class="active">PrmGlobal</li>
                    </ol>
                    <div class="row  mb55">
                        <div class="col-md-12">
                            <a class="btn btn-warning pull-right btn-sm" href='@Url.Action("Index")'><i class="fa fa-arrow-left"></i> Back To List</a>
                            <div class="clearfix"></div>
                            <hr />
                            <div class="panel panel-light-blue">
                                <div class="panel-heading"> <i class="fa fa-plus-circle"></i> <strong>ADD</strong> </div>
                                <div class="panel-body">
                                    @using (Html.BeginForm())
                                    {
                                        @Html.AntiForgeryToken()
                                        @Html.ValidationSummary(true)
                                        <div class="form-group col-md-3">
                                            @Html.LabelFor(model => model.prmmodule, null, "")
                                            @Html.TextBoxFor(model => model.prmmodule, new { @class = "form-control alphanumspacecolon", @maxlength = "30" })
                                            @Html.ValidationMessageFor(model => model.prmmodule, null, new { @class = "validation-redcolor " })
                                        </div>
                                        <div class="form-group col-md-3">
                                            @Html.LabelFor(model => model.prmunit, null, "")
                                            @Html.TextBoxFor(model => model.prmunit, new { @class = "form-control alpha", @maxlength = "15" })
                                            @Html.ValidationMessageFor(model => model.prmunit, null, new { @class = "validation-redcolor" })
                                        </div>
                                        <div class="form-group col-md-3">
                                            @Html.LabelFor(model => model.prmidentifier, null, "")
                                            @Html.TextBoxFor(model => model.prmidentifier, new { @class = "form-control alpha", @maxlength = "15" })
                                            @Html.ValidationMessageFor(model => model.prmidentifier, null, new { @class = "validation-redcolor" })
                                        </div>
                                        
                                        <div class="form-group col-md-3">
                                            @Html.LabelFor(model => model.prmvalue, null, "")
                                            @Html.TextBoxFor(model => model.prmvalue, new { @class = "form-control alpha", @maxlength = "15" })
                                            @Html.ValidationMessageFor(model => model.prmvalue, null, new { @class = "validation-redcolor" })
                                        </div>
                                      
                                        <div class="clearfix"></div>
                                       
                                     
                                       
                                        <div class="panel-footer col-md-12 text-center">
                                            <button type="submit" class="btn btn-sm btn-success" id="btnCreate"><i class="fa fa-save"></i>&nbsp;&nbsp;Save</button>
                                            <button type="reset" class="btn btn-sm btn-danger" id="btnReset"><i class="fa fa-undo"></i>&nbsp;&nbsp;Reset</button>
                                        </div>
                                    }
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</section>
@Scripts.Render("~/bundles/jqueryval")


