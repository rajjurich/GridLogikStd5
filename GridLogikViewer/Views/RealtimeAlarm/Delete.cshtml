                                        @Html.AntiForgeryToken()

                                        <div class="form-horizontal">

                                            @Html.ValidationSummary(true)
                                            @Html.HiddenFor(model => model.id)

                                            <div class="form-group">
                                                    <label class="control-label col-md-2">Enter Alarm Name</label>
