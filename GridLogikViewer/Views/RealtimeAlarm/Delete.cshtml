                                                                        <div class="bg-white">
                                                                            @Html.DropDownList("Parameter", (IEnumerable<SelectListItem>)ViewBag.parame, new { @id = "ddlparameter", @class = "form-control", @multiple = "multiple", @size = "7", @disabled = "disabled" })
