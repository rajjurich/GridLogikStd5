                                                                                @Html.DropDownListFor(model => model.multiplemeterID, Enumerable.Empty<SelectListItem>(), new { @id = "ddlmeterselect", @class = "form-control", @multiple = "multiple", @size = "7", @disabled = "disabled" })



                                                                            </div>
                                                                        </div>
                                                                    </div>
                                                                </div>

                                                                <!--/col-->

                                                            </div>
                                                        </div>
                                                        </div>

                                                        <div class="col-md-6">
