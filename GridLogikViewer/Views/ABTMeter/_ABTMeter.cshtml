@model GridLogikViewer.APIModels.InstanceData

@*//kunal*@
<div class="col-lg-6" style="border:0px solid  #000; padding:0px;">
    <div class="col-lg-12" style="border:0px solid  #000; ">
        <div class="panel panel-default">
            <div class="panel-heading">
                <div class=" dashboard-panel-body-box">Energy</div>
                <div class="panel-actions  dashboard-panel-body-boxcontrols"> <a href="charts-flot.html#" class="btn-setting" data-toggle="modal" data-target="#ModalTable01"><i class="fa fa-eye"></i></a> <a href="charts-flot.html#" class="btn-minimize"><i class="fa fa-chevron-up"></i></a> <a href="charts-flot.html#" class="btn-close"><i class="fa fa-times"></i></a> </div>
            </div>
            <table class="panel-body table table-striped table-bordered table-hover ">
                <thead>
                    <tr>
                        <th class="huge_size_level2">#</th>
                        <th class="huge_size_level2" style="font-size:18px;">Export</th>
                        <th class="huge_size_level2" style="font-size:18px;">Import</th>
                    </tr>
                </thead>
                <tbody>
                    <tr class="info">
                        <th class="huge_size_level2">MWh</th>
                        <td class="huge_size_level3">@Html.DisplayFor(m => m.kWh_Exp)</td>
                        <td class="huge_size_level3">@Html.DisplayFor(m => m.kWh_Imp)</td>
                    </tr>
                    <tr class="warning">
                        <th class="huge_size_level2">MVARh Lag</th>
                        <td class="huge_size_level3">@Html.DisplayFor(m => m.kVARh_Lag_Exp)</td>
                        <td class="huge_size_level3">@Html.DisplayFor(m => m.kVARh_Lag_Imp)</td>
                    </tr>
                    <tr class="danger">
                        <th class="huge_size_level2">MVARh Lead</th>
                        <td class="huge_size_level3">@Html.DisplayFor(m => m.KVARh_Lead_exp)</td>
                        <td class="huge_size_level3">@Html.DisplayFor(m => m.kVARh_Lead_Imp)</td>
                    </tr>
                    <tr class="success">
                        <th class="huge_size_level2">MVAh</th>
                        <td class="huge_size_level3">@Html.DisplayFor(m => m.kVAh_Exp)</td>
                        <td class="huge_size_level3">@Html.DisplayFor(m => m.kVAh_Imp)</td>
                    </tr>
                </tbody>
            </table>

            <!-- Modal03 popup -->
            <!--/col-->

        </div>
    </div>


    <div class="col-lg-12" style="border:0px solid  #000; ">
        <div class="panel panel-default">
