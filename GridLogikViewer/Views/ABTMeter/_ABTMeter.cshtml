                    </tr>

                    <tr class="info">
                        <th class="huge_size_level2">HZ</th>
                        <td class="huge_size_level3">@Html.DisplayFor(m => m.HZ)</td>
                    </tr>
                </tbody>
            </table>

            <!-- Modal03 popup -->
            <!--/col-->

        </div>
    </div>




