                    <a href="charts-flot.html#" class="btn-close"><i class="fa fa-times"></i></a>
                </div>
            </div>
            <table class="panel-body table table-striped table-bordered table-hover ">
                <thead>
                    <tr>
                        <th class="huge_size_level2" style="font-size:18px;">Data</th>
                        <th class="huge_size_level2" style="font-size:18px;">Value</th>

                    </tr>
                </thead>
                <tbody>
                    <tr class="info">
                        <th class="huge_size_level2">MW</th>
                        <td class="huge_size_level3">@Html.DisplayFor(m => m.kW)</td>

                    </tr>
                    <tr class="warning">
                        <th class="huge_size_level2">MVAR </th>
                        <td class="huge_size_level3">@Html.DisplayFor(m => m.kVAR)</td>
                    </tr>

                    <tr class="danger">
                        <th class="huge_size_level2">MVA </th>
