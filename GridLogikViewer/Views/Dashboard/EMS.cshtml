@{
    ViewBag.Title = "EMS";
}

<div class="panel panel-default">
    <div class="panel-heading"> Select Meter  </div>
    <div class="dashboard-selectoption-panel-body">
        <div class="dashboard-form-optionbox">
            <img src="~/Content/images/icon_img/metertype_icon.png" />
            <div class="dashboard-selectoption">
                <select name="sport" id="sport" class="validate[required] form-control">
                    <option value="">Select Meter Type</option>
                    <option value="#"></option>
                    <option value="option1">ABT</option>
                    <option value="option2">EMS</option>
                </select>
            </div>
        </div>
        <div class="dashboard-form-optionbox">
            <img src="~/Content/images/icon_img/metergroup_icon.png" />
            <div class="dashboard-selectoption">
                <select name="sport" id="sport" class="validate[required] form-control">
                    <option value="">Select Meter Group</option>
                    <option value="#"></option>
                    <option value="option1">a</option>
                    <option value="option2">b</option>
                    <option value="option3">c</option>
                </select>
            </div>
        </div>
        <div class="dashboard-form-optionbox">
            <img src="~/Content/images/icon_img/metername_icon.png" />
            <div class="dashboard-selectoption">
                <select name="sport" id="sport" class="validate[required] form-control input-small shadow_select">
                    <option value="">Select Meter Name</option>
                    <option value="#"></option>
                    <option value="option1">x</option>
                    <option value="option2">y</option>
                    <option value="option3">z</option>
                </select>
            </div>
        </div>
        <div class="dashboard-form-optionbox-submitbtn">
            <a href="#" class="dashboard-form-optionbox-submit-btn">Submit</a>
        </div>
    </div>
</div>

