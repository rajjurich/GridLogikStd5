@{
    ViewBag.Title = "Daily Report";
}
<style>
    .ui-datepicker {
        z-index: 10000000 !important;
    }
</style>
<script>
    $(document).ready(function () {

        @*if ('@TempData["drmsg"]' != '') {
            alert('@TempData["drmsg"]', '', false, 'S')
        }*@

        $("#FromDate").datepicker({
            changeMonth: true,
            changeYear: true,
            maxDate: '0'

        }).datepicker('setDate', new Date());

        //$("#ToDate").datepicker({
        //    changeMonth: true,
        //    changeYear: true,
        //    maxDate: '0'

        //});

        $("#btnExport").click(function () {
            //debugger;
            var fromDate = new Date($("#FromDate").val());
            var day1 = fromDate.getDate();
            var month1 = fromDate.getMonth() + 1;
            var year1 = fromDate.getFullYear();

            window.location.href = '/RealTimeABTDisplay/GetDailyReport?day=' + day1 + '&month=' + month1 + '&year=' + year1 + '&parameter=' + $("#ddlParameter").val();

        });

    });
   
</script>

