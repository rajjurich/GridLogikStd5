                        <td>@Html.Raw("8.54")</td>
                        <td>@Html.Raw("8.88")</td>
                    </tr>
                    <tr>
                        <td class="cell">@Html.Label("ACTUAL")</td>
                        <td>@Html.Label("9")</td>
                        <td>@Html.Label("9")</td>
                        <td>@Html.Label("9")</td>
                        <td>@Html.Label("9")</td>
                    </tr>
                    <tr>
                        <td rowspan="2" class="cell">@Html.Label("JUINAGAR") </td>
                        <td class="cell">@Html.Label("FORECAST")</td>
                        <td>@Html.Raw("6.012")</td>
                        <td>@Html.Raw("5.98")</td>
                        <td>@Html.Raw("6.12")</td>
