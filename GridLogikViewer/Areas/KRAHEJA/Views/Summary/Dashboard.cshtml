@{
    Layout = "~/Views/Shared/_EToolsLayout1.cshtml";
}


<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    @*<title>Dashboard</title>*@
    <style>
        .tbl_design {
            height: 300px;
            width: 750px;
            text-align: center;
            background-color: #5DADE2;
            border: 1px solid black;
            border-collapse: collapse;
            text-transform: uppercase;
            font-family: Arial, sans-serif;
            font-size: 13px;
            color: white;
            font-weight: bold;
            margin: 0 auto;
        }

            .tbl_design td, .tbl_design th {
                border: 2px solid black;
                border-collapse: collapse;
                height: 20px;
                /*line-height: 20px;*/
            }
            .tbl_design th {
                line-height: 25px;
            }
                
                 .tbl_design tbody tr:nth-child(2n-1) {
                    background-color: #5DADE2;
                }

            .tbl_design tbody tr:hover {
                background-color: #5DADE2;
            }
        .cell {
            color: #1f475f;
        }
    </style>
</head>
<body>
    <div class="panel panel-default">
        <div class="panel-heading">
            <div class=" dashboard-panel-body-box"><h3>Management Dashboard</h3></div>
            <div class="panel-actions  dashboard-panel-body-boxcontrols">
            </div>
        </div>
        <div class="panel-body">

            <table class="tbl_design">
                <thead>
                    <tr>
                        <th style="width:28%;">@Html.Label("LOCATION")</th>
                        <th style="width:18%;">@Html.Label("SERENE")</th>
                        <th style="width:18%;">@Html.Label("GIGAPLEX")</th>
                        <th style="width:18%;">@Html.Label("HYDERABAD")</th>
                        <th style="width:18%;">@Html.Label("JUINAGAR")</th>
                    </tr>
                </thead>
                <tbody>

                    <tr>
                        <td class="cell">@Html.Label("VLL")</td>
                        <td>@Html.Label("422")</td>
                        <td>@Html.Label("430")</td>
                        <td>@Html.Label("425")</td>
                        <td>@Html.Label("421")</td>
                    </tr>
                    <tr>
                        <td class="cell">@Html.Label("I")</td>
                        <td>@Html.Label("370")</td>
                        <td>@Html.Label("110")</td>
                        <td>@Html.Label("220")</td>
                        <td>@Html.Label("159")</td>
                    </tr>
                    <tr>
                        <td class="cell">@Html.Label("MW") </td>
                        <td>@Html.Raw("13.234")</td>
                        <td>@Html.Raw("3.45")</td>
                        <td>@Html.Raw("7.905")</td>
                        <td>@Html.Raw("5.872")</td>
                    </tr>
                    <tr>
                        <td class="cell">@Html.Label("KWH")</td>
                        <td>@Html.Raw("432678.1")</td>
                        <td>@Html.Raw("765431.8")</td>
                        <td>@Html.Raw("123456.9")</td>
                        <td>@Html.Raw("654321.9")</td>
                    </tr>
                    <tr>
                        <td class="cell">@Html.Label("FORECAST VALUE (MW)")</td>
                        <td>@Html.Label("13")</td>
                        <td>@Html.Label("3")</td>
                        <td>@Html.Label("8")</td>
                        <td>@Html.Label("6")</td>
                    </tr>
                    <tr>
                        <td class="cell">@Html.Label("SCHEDULE DRAWL (MW)")</td>
                        <td>@Html.Label("13")</td>
                        <td>@Html.Label("3")</td>
                        <td>@Html.Label("8")</td>
                        <td>@Html.Label("6")</td>
                    </tr>
                    <tr>
                        <td class="cell">@Html.Label("CHARGES (Rs)")</td>
                        <td>@Html.Raw("2340.2")</td>
                        <td>@Html.Raw("7890.5")</td>
                        <td>@Html.Raw("3451.8")</td>
                        <td>@Html.Raw("4907.9")</td>
                    </tr>
                    <tr>
                        <td class="cell">@Html.Label("LAST BLOCK (MW)")</td>
