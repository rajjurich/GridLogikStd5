                        <td>@Html.Label("3")</td>
                        <td>@Html.Label("3")</td>
                        <td>@Html.Label("3")</td>
                    </tr>
                    <tr>
                        <td rowspan="2" class="cell">@Html.Label("HYDERABAD") </td>
                        <td class="cell">@Html.Label("FORECAST")</td>
                        <td>@Html.Raw("8.98")</td>
